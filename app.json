{
  "expo": {
    "name": "virtual-home-organizer",
    "slug": "virtual-home-organizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow this app to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow this app to access your imag."
        }
      ],
      "expo-localization"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode":  "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zighia.virtualhomeorganizer",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.zighia.virtualhomeorganizer",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d7cbf615-1ac5-4e29-9d1c-6c33836f6321"
      }
    },
    "owner": "zighia",
    "updates": {
      "url": "https://u.expo.dev/d7cbf615-1ac5-4e29-9d1c-6c33836f6321"
    }
  }
}
